
.content {
	width: 750rpx;
	height: 155rpx;
	background-color: #eeeeee;
}
.bar-view {
	width: 100%;
	height: 155rpx;
	text-align: center;
	white-space: nowrap;
}
.titleBar {
	display: flex;
	flex-direction: row;
	align-items: center;
	padding-left: 110rpx;
	padding-top: 77rpx;
}
.barItem {
	display: flex;
	flex-direction: column;
	margin-left: 70rpx;
	font-size: 30rpx;
}
.bottomLine {
	width: 77rpx;
	height: 10rpx;
	background-color: #4778fa;
	border-radius: 10rpx;
	margin-top: 30rpx;
	position: absolute;
}
.bottomLine2 {
	width: 77rpx;
	height: 10rpx;
	border-radius: 10rpx;
	margin-top: 0rpx;
	position: absolute;
}
.textBar {
	z-index: 100;
	color: black;
	font-weight: bold;
	margin-left: 8rpx;
}
.textBar2 {
	z-index: 100;
	color: #989898;
	margin-left: 8rpx;
}



